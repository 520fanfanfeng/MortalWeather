<!--litepal的配置文件-->
<litepal>

    <!--将数据库指定为mortal_weather-->
    <dbname value="mortal_weather" />
    <!--将版本号指定为1-->
    <version value="1" />
    <!--将3个实体类Province、City和County添加到映射列表当中-->
    <list>
        <mapping class="com.example.mortalweather.db.Province" />
        <mapping class="com.example.mortalweather.db.City" />
        <mapping class="com.example.mortalweather.db.County" />
    </list>

</litepal>
